{
	"manifest_version": 2.0,
	"id": "problemsbysvmnz",
	"type": "widget",
	"name": "Problems by severity Monzphere",
	"namespace": "ProblemsBySvMnz",
	"version": "1.0",
	"author": "Zabbix",
	"description": "Displays the problem count by severity.",
	"url": "web_interface/frontend_sections/dashboards/widgets/problems_severity",
	"widget": {
		"js_class": "CWidgetProblemsBySvMnz",
		"in": {
			"hostids": {
				"type": "_hostids"
			},
			"groupids": {
				"type": "_hostgroupids"
			}
		},
		"out": [
			{
				"type": "_hostgroupid"
			},
			{
				"type": "_hostgroupids"
			}
		]
	},
	"actions": {
		"widget.problemsbysvmnz.view": {
			"class": "WidgetView"
		}
	},
	"assets": {
		"js": ["class.widget.js"]
	}
}
